 */
public class XMLHandler implements InvocationHandler {

	@Override
	public Object invoke(Object proxy, Method method, Object[] args)
			throws Throwable {
		if(method.getName().equals("writeXML")){
			
		}else if(method.getName().equals("readXML")){
			
		}		
		return null;
	}
}
